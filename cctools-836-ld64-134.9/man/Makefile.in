builddir = @builddir@
top_builddir = @top_builddir@
srcdir = @srcdir@
top_srcdir = @top_srcdir@
datarootdir = @datarootdir@
prefix	= @prefix@
exec_prefix	= @exec_prefix@
mandir		= @mandir@
includedir	= @includedir@

transform   = @program_transform_name@

VPATH = @srcdir@

INSTALL		= @INSTALL@
INSTALL_PROGRAM = @INSTALL_PROGRAM@
INSTALL_DATA	= @INSTALL_DATA@

MANPAGES1	= as.1 checksyms.1 codesign_allocate.1 indr.1 install_name_tool.1 libtool.1  lipo.1 nm.1 nmedit.1 otool.1 pagestuff.1 ranlib.1 redo_prebinding.1 seg_addr_table.1 segedit.1 size.1 strings.1 strip.1

MANPAGES3	= 

MANPAGES5	= 

.PHONY: default install clean distclean

default:

install: 
	mkdir -p $(DESTDIR)/$(mandir)
	mkdir -p $(DESTDIR)/$(mandir)/man1
	mkdir -p $(DESTDIR)/$(mandir)/man3
	mkdir -p $(DESTDIR)/$(mandir)/man5
	for m in $(MANPAGES1); do				\
		$(INSTALL_DATA) $(srcdir)/$$m			\
			$(DESTDIR)/$(mandir)/man1/`echo $$m| sed '$(transform)'`;		\
	done
	for m in $(MANPAGES3); do				\
		$(INSTALL_DATA) $(srcdir)/$$m			\
			$(DESTDIR)/$(mandir)/man3/`echo $$m| sed '$(transform)'`;		\
	done
	for m in $(MANPAGES5); do				\
		$(INSTALL_DATA) $(srcdir)/$$m			\
			$(DESTDIR)/$(mandir)/man5/`echo $$m| sed '$(transform)'`;		\
	done


clean:
